{
	"schemaVersion": 1,
	"id": "macraft",
	"version": "${version}",
	"name": "Ma Craft",
	"description": "Add some magic to your minecraft",
	"authors": [
		"Anatoly Speransky"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/modid/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.macraft.MaCraft"
		],
		"client": [
			"com.macraft.MaCraftClient"
		]
	},
	"mixins": [
		"macraft.mixins.json",
		{
			"config": "macraft.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}